package Array3.Ladder1_1210;

import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		for (int t = 0; t < 10; t++) {
			int N = 100;
			sc.nextInt();
			int[][] map = new int[N][N];
			int sj = 0;
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					map[i][j] = sc.nextInt();
					if (map[i][j] == 2)
						sj = j;
				}
			}

			int i = N - 1;
			int j = sj;
			int left = j - 1;
			int right = j + 1;
			while (i >= 0) {
				System.out.println(i);
				if (0 <= left && map[i][left] == 1) {
					while (0 <= left - 1 && map[i][left -  1] == 1) {
						left--;
					}
					while (0 <= left - 1 && map[i][left - 1] == 0 && left + 1 < N && map[i][left + 1] == 0) {
						i--;
					}

				} else if (right < N && map[i][right] == 1) {
					while (right + 1 < N && map[i][right + 1] == 1) {
						right++;
					}
					while (0 <= right - 1 && map[i][right - 1] == 0 && right + 1 < N && map[i][right + 1] == 0) {
						i--;
					}

				}
			}
			System.out.printf("#%d %d\n", t + 1, j);

		}

		sc.close();

	}
}
