package 백트래킹.규영이와_인영이의_카드게임;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;
import java.util.stream.IntStream;

public class Solution {
	public static List<Integer> deck;
	public static int N = 9;
	public static List<Integer> result;
	public static int a;
	public static int b;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			deck = new ArrayList<>();
			for (int i = 0; i < 9; i++) {
				deck.add(sc.nextInt());
			}
			List<Integer> deck2 = new ArrayList<>();
			IntStream.rangeClosed(1, 18).forEach(deck2::add);
			deck.forEach(deck2::remove);
			System.out.println(t);
			System.out.println(deck);
			System.out.println(deck2);
			
		}
		
		sc.close();
	}
	
	public static void perm(int idx, int visited) {
		if (visited == (1 << N) - 1) {
//			for ()
		}
		
		for (int i = 0; i < N; i++) {
			if ((visited & (1 << i)) > 0) continue;
			result.set(idx, deck.get(i));
			perm(idx + 1, visited | (1 << i));
		}

	}

}