package 요격_시스템;

import java.util.Arrays;

class Solution {
	
	public static void main(String[] args) {
		int[][] targets = {{4,5},{4,8},{10,14},{11,13},{5,12},{3,7},{1,4}};
		solution(targets);
	}
	
    public static int solution(int[][] targets) {
        int answer = 0;
        
        Arrays.sort(targets);
        System.out.println(Arrays.toString(targets));
        
        return answer;
    }
}

