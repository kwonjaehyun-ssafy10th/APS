package IM대비_문제풀이.원재의_메모리_복구하기;

import java.util.Scanner;
 
public class Solution {
 
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         
        int T = sc.nextInt();
        for (int t = 1; t <= T; t++) {
        	int ans = 0;
        	String mem = sc.next();
        	int idx = mem.indexOf('1');
        	for (int i = idx; i < mem.length(); i++) {
        		if (mem.charAt(idx) != mem.charAt(idx - 1)) ans++;
        	}
            
            System.out.println(String.format("#%d %d", t, ans));
        }
         
        sc.close();
    }
 
}