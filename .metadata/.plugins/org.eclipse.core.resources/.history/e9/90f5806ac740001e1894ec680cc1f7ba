package Day2_출력_연산.문자열_돌리기;

import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int eo = n % 2;
        String flag;
        if (eo == 0) flag = "even";
        else flag = "odd";
        System.out.println(String.format("%d is %s", n, flag));
    }
}