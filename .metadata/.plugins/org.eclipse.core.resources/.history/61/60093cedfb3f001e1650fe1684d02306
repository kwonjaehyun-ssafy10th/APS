package Queue2.진기의_최고급_붕어빵_1860;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			int N = sc.nextInt(), M = sc.nextInt(), K = sc.nextInt();
			int timeC = 0;
			int timeB = 0;
			int bun = 0;
			boolean flag = true;
			String ans = "ossible";
			String flagStr = "";
			for (int n = 0; n < N; n++) {
				int sec = sc.nextInt();
				timeC += sec;
				int baked = (timeC - timeB) / K;
				bun += baked;
				timeB += bun * K;
				if (bun > 0) bun--;
				else {
					flag = false;
					break;
				}				
			}
			if (flag) flagStr = "P";
			else flagStr = "Imp";
			System.out.println(String.format("#%d %s%s", t, flagStr , ans));
		}
		sc.close();
	}

}
