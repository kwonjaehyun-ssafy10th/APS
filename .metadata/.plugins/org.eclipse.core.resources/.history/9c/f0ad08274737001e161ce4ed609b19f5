package Array4.오목_판정_11315;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
//		Scanner sc = new Scanner(System.in);
		Scanner sc = new Scanner(input);
		
		int T = sc.nextInt();
		for (int t = 1; t < T; t++) {
			int N = sc.nextInt();
			int[][] map = new int[N][N];
			
			for (int r = 0; r < N; r++) {
				String line = sc.next();
				for (int c = 0; c < N; c++) {
					map[r][c] = line.charAt(c);
				}
			}
			
			String ans = "No";
			boolean flag = false;
			Hori: for (int r = 0; r < N; r++) {
				for (int c = 0; c < N; c++) {
					int cnt = 0;
					countH: for (int k = 0; k < 5; k++) {
						int nc = c + k;
						if (nc < N) {
							if (map[r][nc] == '.') {
									break countH;
							} else cnt++;
						}
						if (cnt == 5) {
							ans = "YES";
							flag = true;
							break Hori;
						}
					}
				}
			}
			if (flag) {
				System.out.printf("#%d %s\n", t, ans);
				continue;
			}
			
			
		}
		
		
		
		sc.close();
	}

	public static String input = "4\r\n" + 
			"5\r\n" + 
			"....o\r\n" + 
			"...o.\r\n" + 
			"..o..\r\n" + 
			".o...\r\n" + 
			"o....\r\n" + 
			"5\r\n" + 
			"...o.\r\n" + 
			"ooooo\r\n" + 
			"...o.\r\n" + 
			"...o.\r\n" + 
			".....\r\n" + 
			"5\r\n" + 
			".o.oo\r\n" + 
			"oo.oo\r\n" + 
			".oo..\r\n" + 
			".o...\r\n" + 
			".o...\r\n" + 
			"5\r\n" + 
			".o.o.\r\n" + 
			"o.o.o\r\n" + 
			".o.o.\r\n" + 
			"o.o.o\r\n" + 
			".o.o.";
}
