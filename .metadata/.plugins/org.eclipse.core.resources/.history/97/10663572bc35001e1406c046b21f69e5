package 영어_끝말잇기;

import java.util.List;

class Solution {
    public int[] solution(int n, String[] words) {
        int[] answer = new int[2];
        
        List<String> used = new ArrayList<>();
        
        for (int i = 0; i < words.length; i++) {
        	if (used.contains(words[i])) {
        		answer[0] = (i % n + 1);
        		answer[1] = (i / n + 1);
        	}
        }

        return answer;
    }
}