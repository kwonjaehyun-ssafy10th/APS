package 집합.햄버거_다이어트_5215;

import java.util.Arrays;
import java.util.Scanner;
 
public class Solution2 {
     
    public static int N, L;
    public static int[][] hamburgers;
    public static int maxScore;
     
    public static void recur(int lasti, int score, int cal) {
        if (cal > L) return;
         
        if (score > maxScore) {
            maxScore = score;
        }
         
        for (int i = lasti + 1; i < N; i++) {
            recur(i, score + hamburgers[i][0], cal + hamburgers[i][1]);
        }
    }
     
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         
        int T = sc.nextInt();
         
        for (int test_case = 1; test_case <= T; test_case++) {
            N = sc.nextInt(); 
            L = sc.nextInt();
             
            hamburgers = new int[N][2];
             
            maxScore = -1;
             
            for (int i = 0; i < N; i++) {
                hamburgers[i][0] = sc.nextInt();
                hamburgers[i][1] = sc.nextInt();
            }
             
            recur(-1, 0, 0);
             
            System.out.println("#" + test_case + " " + maxScore);
        }
    }
}