package Array2.Flatten_1208;

import java.util.Arrays;
import java.util.Scanner;

class Solution {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		for (int t = 1; t <= 10; t++) {
			int dump = sc.nextInt();
			int[] cnt = new int[101];
			Arrays.fill(cnt, 0);
			for (int i = 0; i < 100; i++) {
				cnt[sc.nextInt()]++;
			}
			System.out.println(Arrays.toString(cnt));
			
			int lowest = 1;
			int highest = 100;
			int diff = highest - lowest;
			
			while (dump > 569 && diff > 1) {
				cnt[highest]--;
				cnt[highest - 1]++;
				cnt[lowest]--;
				cnt[lowest + 1]++;
				dump--;
				while (cnt[lowest] == 0 && lowest < 99) {
					lowest++;
				}
				while (cnt[highest] == 0 && highest > 1) {
					highest--;
				}
				diff = highest - lowest;
				System.out.println(String.format("dump: %d", dump));
				System.out.println(String.format("highet: %d, lowest: %d, diff: %d", highest, lowest, diff));
				System.out.println(Arrays.toString(cnt));
				System.out.println();
			}
			
			System.out.println(String.format("#%d %d", t, diff));
			
		}
		
		sc.close();
	}

}
